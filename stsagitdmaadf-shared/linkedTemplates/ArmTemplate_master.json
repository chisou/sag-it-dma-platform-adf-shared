{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "stsagitdmaadf-shared"
		},
		"ConnectToStagingIn_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ConnectToStagingIn'"
		},
		"ConnectToStagingInWrkMarketing_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ConnectToStagingInWrkMarketing'"
		},
		"ConnectionToBlob_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ConnectionToBlob'"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "stsagitdmaadf-shared_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ConnectToStagingIn_connectionString": {
						"value": "[parameters('ConnectToStagingIn_connectionString')]"
					},
					"ConnectToStagingInWrkMarketing_connectionString": {
						"value": "[parameters('ConnectToStagingInWrkMarketing_connectionString')]"
					},
					"ConnectionToBlob_connectionString": {
						"value": "[parameters('ConnectionToBlob_connectionString')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "stsagitdmaadf-shared_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'stsagitdmaadf-shared_ArmTemplate_0')]"
			]
		}
	]
}